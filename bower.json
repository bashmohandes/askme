{
  "name": "askme",
  "description": "A web Application to lean node.js and build a replacement of my ask.fm/bashmohandes website that I have better control over",
  "main": "app.js",
  "authors": [
    "Mohamed Elsherif <bashmohandes@live.com>"
  ],
  "license": "MIT",
  "homepage": "https://github.com/bashmohandes/askme",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "bootstrap": "^3.3.7",
